 <h3>スケジュール新規登録</h3>
 
 <%= form_with(model: @schedule, local: true) do |f| %>
      <table>
        <tr>
          <th><%= f.label :title ,"タイトル" %></th>
          <td><%= f.text_field :title %></td>
        </tr>
 
        <tr>
          <th><%= f.label :startdate ,"開始日" %></th>
          <td><%= f.text_field :startdate %></td>
        </tr>
 
        <tr>
          <th><%= f.label :enddate ,"終了日" %></th>
          <td><%= f.text_field :enddate %></td>
        </tr>
 
        <tr>
          <th><%= f.label :endcheck ,"終日" %></th>
          <td><%= f.text_area :endcheak %></td>
        </tr>
        <tr>
          <th><%= f.label :schedulememo ,"スケジュールメモ" %></th>
          <td><%= f.text_area :schedulememo %></td>
        </tr>
      </table>
 
      <div>
        <ul>
          <li><%= f.submit "スケジュールを新規登録する"%></li>
          <li><%= link_to "スケジュール一覧に戻る", :schedules %></li>
        </ul>
      </div>
    <% end %>